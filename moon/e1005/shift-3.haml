  %autogen __KeyToKey__ KeyCode::T, KeyCode::VK_STICKY_SHIFT_L, KeyCode::SLASH
  -#/上段 右手
  -#%autogen __KeyToKey__ KeyCode::Y, ModifierFlag::NONE, KeyCode::Y
  -#%autogen __KeyToKey__ KeyCode::U, ModifierFlag::NONE, KeyCode::U
