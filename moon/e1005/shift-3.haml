  %autogen __KeyToKey__ KeyCode::G, KeyCode::VK_STICKY_SHIFT_L, KeyCode::KEY_6
  -#/中段 右手
  -#%autogen __KeyToKey__ KeyCode::H,         ModifierFlag::NONE, KeyCode::H
  -#%autogen __KeyToKey__ KeyCode::J,         ModifierFlag::NONE, KeyCode::J
