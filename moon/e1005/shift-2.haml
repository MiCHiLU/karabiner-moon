    -#%autogen __HoldingKeyToKey__ KeyCode::BRACKET_RIGHT, ModifierFlag::NONE
    -#%autogen __HoldingKeyToKey__ KeyCode::BACKSLASH,     ModifierFlag::NONE

    /ばじどだぶ|
    /中段 左手
    %autogen __HoldingKeyToKey__ KeyCode::A, ModifierFlag::NONE, KeyCode::F,     KeyCode::BRACKET_LEFT
    %autogen __HoldingKeyToKey__ KeyCode::S, ModifierFlag::NONE, KeyCode::D,     KeyCode::BRACKET_LEFT
    %autogen __HoldingKeyToKey__ KeyCode::D, ModifierFlag::NONE, KeyCode::S,     KeyCode::BRACKET_LEFT
    %autogen __HoldingKeyToKey__ KeyCode::F, ModifierFlag::NONE, KeyCode::Q,     KeyCode::BRACKET_LEFT
    %autogen __HoldingKeyToKey__ KeyCode::G, ModifierFlag::NONE, KeyCode::KEY_2, KeyCode::BRACKET_LEFT
    -#/中段 右手
    -#%autogen __HoldingKeyToKey__ KeyCode::H,         ModifierFlag::NONE, KeyCode::H
    -#%autogen __HoldingKeyToKey__ KeyCode::J,         ModifierFlag::NONE, KeyCode::J
    -#%autogen __HoldingKeyToKey__ KeyCode::K,         ModifierFlag::NONE, KeyCode::K
    -#%autogen __HoldingKeyToKey__ KeyCode::L,         ModifierFlag::NONE, KeyCode::L
    -#%autogen __HoldingKeyToKey__ KeyCode::SEMICOLON, ModifierFlag::NONE, KeyCode::SEMICOLON
    -#%autogen __HoldingKeyToKey__ KeyCode::QUOTE,     ModifierFlag::NONE, KeyCode::QUOTE

    /パピプペポ|
    /下段 左手
    %autogen __HoldingKeyToKey__ KeyCode::Z, ModifierFlag::NONE, KeyCode::F,          KeyCode::EQUAL
    %autogen __HoldingKeyToKey__ KeyCode::X, ModifierFlag::NONE, KeyCode::V,          KeyCode::EQUAL
    %autogen __HoldingKeyToKey__ KeyCode::C, ModifierFlag::NONE, KeyCode::KEY_2,      KeyCode::EQUAL
