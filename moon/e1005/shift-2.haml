    -#/下段 右手
    -#%autogen __HoldingKeyToKey__ KeyCode::N,     ModifierFlag::NONE, KeyCode::N
    -#%autogen __HoldingKeyToKey__ KeyCode::M,     ModifierFlag::NONE, KeyCode::M
    -#%autogen __HoldingKeyToKey__ KeyCode::COMMA, ModifierFlag::NONE, KeyCode::COMMA
    -#%autogen __HoldingKeyToKey__ KeyCode::DOT,   ModifierFlag::NONE, KeyCode::DOT
