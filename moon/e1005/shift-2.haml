    -#%autogen __HoldingKeyToKey__ KeyCode::MINUS, ModifierFlag::NONE, KeyCode::MINUS
    -#%autogen __HoldingKeyToKey__ KeyCode::EQUAL, ModifierFlag::NONE, KeyCode::EQUAL

    /ざぎずぜぞ|
    /上段 左手
    %autogen __HoldingKeyToKey__ KeyCode::Q, ModifierFlag::NONE, KeyCode::X, KeyCode::BRACKET_LEFT
    %autogen __HoldingKeyToKey__ KeyCode::W, ModifierFlag::NONE, KeyCode::G, KeyCode::BRACKET_LEFT
    %autogen __HoldingKeyToKey__ KeyCode::E, ModifierFlag::NONE, KeyCode::R, KeyCode::BRACKET_LEFT
    %autogen __HoldingKeyToKey__ KeyCode::R, ModifierFlag::NONE, KeyCode::P, KeyCode::BRACKET_LEFT
    %autogen __HoldingKeyToKey__ KeyCode::T, ModifierFlag::NONE, KeyCode::C, KeyCode::BRACKET_LEFT
    -#/上段 右手
    -#%autogen __HoldingKeyToKey__ KeyCode::Y, ModifierFlag::NONE, KeyCode::Y
    -#%autogen __HoldingKeyToKey__ KeyCode::U, ModifierFlag::NONE, KeyCode::U
    -#%autogen __HoldingKeyToKey__ KeyCode::I, ModifierFlag::NONE, KeyCode::I
    -#%autogen __HoldingKeyToKey__ KeyCode::O, ModifierFlag::NONE, KeyCode::O
