!!! XML
%root
  -#{{JAPANESE_MOON_EXTRA}}
  %inputmode_only JAPANESE
  %not PASSWORD

  /薬指シフト

  -#/上段 左手 数字キー
  -#%autogen __KeyToKey__ KeyCode::BACKQUOTE, ModifierFlag::OPTION_L, KeyCode::BACKQUOTE
  -#%autogen __KeyToKey__ KeyCode::KEY_1,     ModifierFlag::OPTION_L, KeyCode::KEY_1
  -#%autogen __KeyToKey__ KeyCode::KEY_2,     ModifierFlag::OPTION_L, KeyCode::KEY_2
  -#%autogen __KeyToKey__ KeyCode::KEY_3,     ModifierFlag::OPTION_L, KeyCode::KEY_3
  -#%autogen __KeyToKey__ KeyCode::KEY_4,     ModifierFlag::OPTION_L, KeyCode::KEY_4
  -#%autogen __KeyToKey__ KeyCode::KEY_5,     ModifierFlag::OPTION_L, KeyCode::KEY_5
  -#/上段 右手 数字キー
  -#%autogen __KeyToKey__ KeyCode::KEY_6, ModifierFlag::OPTION_L, KeyCode::KEY_6
  -#%autogen __KeyToKey__ KeyCode::KEY_7, ModifierFlag::OPTION_L, KeyCode::KEY_7
  -#%autogen __KeyToKey__ KeyCode::KEY_8, ModifierFlag::OPTION_L, KeyCode::KEY_8
  -#%autogen __KeyToKey__ KeyCode::KEY_9, ModifierFlag::OPTION_L, KeyCode::KEY_9
  -#%autogen __KeyToKey__ KeyCode::KEY_0, ModifierFlag::OPTION_L, KeyCode::KEY_0
  -#%autogen __KeyToKey__ KeyCode::MINUS, ModifierFlag::OPTION_L, KeyCode::MINUS
  -#%autogen __KeyToKey__ KeyCode::EQUAL, ModifierFlag::OPTION_L, KeyCode::EQUAL

  /ざぎずぜぞ|
  /上段 左手
  %autogen __KeyToKey__ KeyCode::Q, ModifierFlag::OPTION_L, KeyCode::X, KeyCode::BRACKET_LEFT
  %autogen __KeyToKey__ KeyCode::W, ModifierFlag::OPTION_L, KeyCode::G, KeyCode::BRACKET_LEFT
  %autogen __KeyToKey__ KeyCode::E, ModifierFlag::OPTION_L, KeyCode::R, KeyCode::BRACKET_LEFT
  %autogen __KeyToKey__ KeyCode::R, ModifierFlag::OPTION_L, KeyCode::P, KeyCode::BRACKET_LEFT
  %autogen __KeyToKey__ KeyCode::T, ModifierFlag::OPTION_L, KeyCode::C, KeyCode::BRACKET_LEFT
  -#/上段 右手
  -#%autogen __KeyToKey__ KeyCode::Y, ModifierFlag::OPTION_L, KeyCode::Y
  -#%autogen __KeyToKey__ KeyCode::U, ModifierFlag::OPTION_L, KeyCode::U
  -#%autogen __KeyToKey__ KeyCode::I, ModifierFlag::OPTION_L, KeyCode::I
  -#%autogen __KeyToKey__ KeyCode::O, ModifierFlag::OPTION_L, KeyCode::O
  -#%autogen __KeyToKey__ KeyCode::P, ModifierFlag::OPTION_L, KeyCode::P
  -#%autogen __KeyToKey__ KeyCode::BRACKET_LEFT, ModifierFlag::OPTION_L, KeyCode::BRACKET_LEFT
  -#%autogen __KeyToKey__ KeyCode::BRACKET_RIGHT, ModifierFlag::OPTION_L
  -#%autogen __KeyToKey__ KeyCode::BACKSLASH,     ModifierFlag::OPTION_L

  /ばじどだぶ|
  /中段 左手
  %autogen __KeyToKey__ KeyCode::A, ModifierFlag::OPTION_L, KeyCode::F,     KeyCode::BRACKET_LEFT
  %autogen __KeyToKey__ KeyCode::S, ModifierFlag::OPTION_L, KeyCode::D,     KeyCode::BRACKET_LEFT
  %autogen __KeyToKey__ KeyCode::D, ModifierFlag::OPTION_L, KeyCode::S,     KeyCode::BRACKET_LEFT
  %autogen __KeyToKey__ KeyCode::F, ModifierFlag::OPTION_L, KeyCode::Q,     KeyCode::BRACKET_LEFT
  %autogen __KeyToKey__ KeyCode::G, ModifierFlag::OPTION_L, KeyCode::KEY_2, KeyCode::BRACKET_LEFT
  -#/中段 右手
  -#%autogen __KeyToKey__ KeyCode::H,         ModifierFlag::OPTION_L, KeyCode::H
  -#%autogen __KeyToKey__ KeyCode::J,         ModifierFlag::OPTION_L, KeyCode::J
  -#%autogen __KeyToKey__ KeyCode::K,         ModifierFlag::OPTION_L, KeyCode::K
  -#%autogen __KeyToKey__ KeyCode::L,         ModifierFlag::OPTION_L, KeyCode::L
  -#%autogen __KeyToKey__ KeyCode::SEMICOLON, ModifierFlag::OPTION_L, KeyCode::SEMICOLON
  -#%autogen __KeyToKey__ KeyCode::QUOTE,     ModifierFlag::OPTION_L, KeyCode::QUOTE

  /パピプペポ|
  /下段 左手
  %autogen __KeyToKey__ KeyCode::Z, ModifierFlag::OPTION_L, KeyCode::F,          KeyCode::EQUAL
  %autogen __KeyToKey__ KeyCode::X, ModifierFlag::OPTION_L, KeyCode::V,          KeyCode::EQUAL
  %autogen __KeyToKey__ KeyCode::C, ModifierFlag::OPTION_L, KeyCode::KEY_2,      KeyCode::EQUAL
  %autogen __KeyToKey__ KeyCode::V, ModifierFlag::OPTION_L, KeyCode::BACKSLASH,  KeyCode::EQUAL
  %autogen __KeyToKey__ KeyCode::B, ModifierFlag::OPTION_L, KeyCode::MINUS,      KeyCode::EQUAL
  -#/下段 右手
  -#%autogen __KeyToKey__ KeyCode::N,     ModifierFlag::OPTION_L, KeyCode::N
  -#%autogen __KeyToKey__ KeyCode::M,     ModifierFlag::OPTION_L, KeyCode::M
  -#%autogen __KeyToKey__ KeyCode::COMMA, ModifierFlag::OPTION_L, KeyCode::COMMA
  -#%autogen __KeyToKey__ KeyCode::DOT,   ModifierFlag::OPTION_L, KeyCode::DOT
  -#%autogen __KeyToKey__ KeyCode::SLASH, ModifierFlag::OPTION_L, KeyCode::SLASH
